<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<TabPane prefHeight="414.0" prefWidth="238.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.PatientTabViewController">
   <tabs>
      <Tab text="I miei pazienti">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" layoutY="-5.0" prefHeight="391.0" prefWidth="238.0">
                     <children>
                        <ScrollPane prefHeight="322.0" prefWidth="237.0">
                           <content>
                              <ListView fx:id="patientsListView" prefHeight="310.0" prefWidth="238.0" />
                           </content>
                        </ScrollPane>
                        <Button fx:id="visualizeButton" mnemonicParsing="false" onAction="#visualize" text="visualize">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </AnchorPane>
         </content>
      </Tab>
      <Tab text="Tutti i pazienti">
         <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <VBox alignment="CENTER" layoutY="-3.0" prefHeight="384.0" prefWidth="238.0">
                     <children>
                        <ScrollPane prefHeight="322.0" prefWidth="237.0">
                           <content>
                              <ListView fx:id="allPatientsListView" prefHeight="310.0" prefWidth="238.0" />
                           </content>
                        </ScrollPane>
                        <Button fx:id="visualizeButton1" mnemonicParsing="false" onAction="#visualize" text="visualize">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children></AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
